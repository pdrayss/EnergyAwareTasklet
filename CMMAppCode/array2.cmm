int current, counter;
int[100000000] aArray;
int[100000000] bArray;
int[100000000] cArray;

>>current;
counter := 0;

while(current < 100000000){
	aArray[current] := current;
	bArray[current] := current;
	cArray[current] := current;
	current := current + 1;
	
	if(current % 42 = 0){
		counter := counter +1;
	}
	
}

<<current;
<<counter;